echo "
apiVersion: v1
kind: Secret
metadata:
  name: foo-secret
data:
  tls.crt: '`base64 /tmp/tls.crt`'
  tls.key: '`base64 /tmp/tls.key`'
" | kubectl create -f -


server 172.16.32.7:80;
#Run as root user
echo "1.Checking user..."
echo "1.正在检测用户..."
sleep 1
if [ $UID = 0 ];then
	echo -e "\033[32m\tAll good...\033[0m"
	echo -e "\033[32m\t权限检测正确...\033[0m"
else
	echo -e "\033[31m\tYou must run as root.\033[0m"
	echo -e "\033[31m\t请用root用户执行此脚本。\033[0m"
	exit
fi

sleep 1


ssh-rsa  fang.bo@vpclub.cn
